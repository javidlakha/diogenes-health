          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
        <w:t>No observations recorded.</w:t>
      </w:r>
    </w:p>
        {% endfor %}
    {%- elif section.type == 'text' -%}
