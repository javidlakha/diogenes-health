          <w:color w:val="102A43" />
          <w:sz w:val="36" />
          <w:szCs w:val="36" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="102A43" />
          <w:sz w:val="36" />
          <w:szCs w:val="36" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
        <w:t>{{ patient_identifier }}</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="102A43" />
          <w:sz w:val="28" />
          <w:szCs w:val="28" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="102A43" />
          <w:sz w:val="28" />
          <w:szCs w:val="28" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
        <w:t>{{ name }}</w:t>
      </w:r>
    </w:p>
    {% for section in sections %}
    <w:p>
      <w:pPr>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="102A43" />
          <w:sz w:val="27" />
          <w:szCs w:val="27" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
      </w:pPr>
    </w:p>
    <w:p>
      <w:pPr>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="102A43" />
          <w:sz w:val="28" />
          <w:szCs w:val="28" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="102A43" />
          <w:sz w:val="28" />
          <w:szCs w:val="28" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
        <w:t>{{ section.name }}</w:t>
      </w:r>
    </w:p>
    {% if section.type == 'checklist' -%}
        {% for option in section.options if option.option_identifier in section.checked -%}
    <w:p>
      <w:pPr>
        <w:pStyle w:val="ListParagraph" />
        <w:numPr>
          <w:ilvl w:val="0" />
          <w:numId w:val="2" />
        </w:numPr>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
        <w:t>{{ option.name }}</w:t>
      </w:r>
    </w:p>
        {% else %}
    <w:p>
      <w:pPr>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
        <w:t>No observations recorded.</w:t>
      </w:r>
    </w:p>
        {% endfor %}
    {%- elif section.type == 'diagram' -%}
    <w:p>
      <w:pPr>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI"
            w:cs="Segoe UI" />
          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI"
            w:cs="Segoe UI" />
          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
        <w:drawing>
          <wp:inline distT="0" distB="0" distL="0" distR="0">
            <wp:extent cx="3683000" cy="3708400" />
            <wp:effectExtent l="0" t="0" r="0" b="0" />
            <wp:docPr id="1" name="{{ section.name }}" />
            <wp:cNvGraphicFramePr>
              <a:graphicFrameLocks xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main"
                noChangeAspect="1" />
            </wp:cNvGraphicFramePr>
            <a:graphic xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main">
              <a:graphicData uri="http://schemas.openxmlformats.org/drawingml/2006/picture">
                <pic:pic xmlns:pic="http://schemas.openxmlformats.org/drawingml/2006/picture">
                  <pic:nvPicPr>
                    <pic:cNvPr id="1" name="{{ section.name }}" />
                    <pic:cNvPicPr />
                  </pic:nvPicPr>
                  <pic:blipFill>
                    <a:blip r:embed="rId{{ section.png }}">
                      <a:extLst>
                        <a:ext uri="{28A0092B-C50C-407E-A947-70E740481C1C}">
                          <a14:useLocalDpi
                            xmlns:a14="http://schemas.microsoft.com/office/drawing/2010/main"
                            val="0" />
                        </a:ext>
                      </a:extLst>
                    </a:blip>
                    <a:stretch>
                      <a:fillRect />
                    </a:stretch>
                  </pic:blipFill>
                  <pic:spPr>
                    <a:xfrm>
                      <a:off x="0" y="0" />
                      <a:ext cx="3683000" cy="3708400" />
                    </a:xfrm>
                    <a:prstGeom prst="rect">
                      <a:avLst />
                    </a:prstGeom>
                  </pic:spPr>
                </pic:pic>
              </a:graphicData>
            </a:graphic>
          </wp:inline>
        </w:drawing>
      </w:r>
    </w:p>
    {%- elif section.type == 'single_selection' -%}
        {% for option in section.options if option.option_identifier == section.selected -%}
    <w:p>
      <w:pPr>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="Segoe UI" w:eastAsia="Times New Roman" w:hAnsi="Segoe UI" w:cs="Segoe UI" />
          <w:color w:val="4A5058" />
          <w:lang w:eastAsia="en-GB" />
        </w:rPr>
        <w:t>{{ option.name }}</w:t>
      </w:r>
    </w:p>
        {% else %}
    <w:p>
      <w:pPr>
        <w:rPr>
